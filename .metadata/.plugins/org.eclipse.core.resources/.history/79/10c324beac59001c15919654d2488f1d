package com.dbs.exception;

public class InvalidEmployee extends Exception {

	public InvalidEmployee() {
		// TODO Auto-generated constructor stub
	}

	public InvalidEmployee(String message) {
		super(message);
		// TODO Auto-generated constructor stub
	}

	public InvalidEmployee(Throwable cause) {
		super(cause);
		// TODO Auto-generated constructor stub
	}

	public InvalidEmployee(String message, Throwable cause) {
		super(message, cause);
		// TODO Auto-generated constructor stub
	}

	public InvalidEmployee(String message, Throwable cause, boolean enableSuppression, boolean writableStackTrace) {
		super(message, cause, enableSuppression, writableStackTrace);
		// TODO Auto-generated constructor stub
	}

}
